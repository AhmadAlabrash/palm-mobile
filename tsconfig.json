{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "palm-core/*": ["./palm-core/*"],
      "palm-core/types": ["./palm-core/types"],
      "palm-core/consts": ["./palm-core/consts"],

      "palm-react/*": ["./palm-react/*"],
      "palm-react-native/*": ["./palm-react-native/*"],

      "consts": ["./consts"],
      "hooks/*": ["./hooks/*"],
      "assets/*": ["./assets/*"],
      "config": ["./config"],
      "components": ["./components"]
    },
    "types": ["react-native", "jest", "styled-components-react-native"],
    "esModuleInterop": true,
    "noImplicitAny": false
  }
}
