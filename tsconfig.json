{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "core/libs/*": ["./core/libs/*"],
      "core/types": ["./core/types"],
      "core/consts": ["./core/consts"],
      "core/graphqls/*": ["./core/graphqls/*"],

      "react/store/*": ["./react/store/*"],

      "reactnative": ["./reactnative"],

      "consts": ["./consts"],
      "hooks/*": ["./hooks/*"],
      "assets/*": ["./assets/*"],
      "config": ["./config"],
      "components": ["./components"]
    },
    "types": ["react-native", "jest", "styled-components-react-native"],
    "esModuleInterop": true,
    "noImplicitAny": false
  }
}
